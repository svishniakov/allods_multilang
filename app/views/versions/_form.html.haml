.panel.panel-default.center
  .panel-heading Upload new version
  .panel-body
    = form_tag(upload_versions_path, :multipart => true, class: "direct-upload") do
      .form-group
        = label_tag(:version, "Choose a file: ")
        = file_field_tag(:version, class: "form-control")
      .form-group
        = submit_tag("Upload", class: "btn btn-primary")
        %br/